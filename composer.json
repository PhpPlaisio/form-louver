{
  "name": "plaisio/form-louver",
  "description": "PhpPlaisio: Louver Controls",
  "keywords": [
    "PhpPlaisio",
    "Form",
    "Louver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio-ts/helper-cast": "^2.0.0",
    "plaisio-ts/kernel": "^2.1.0",
    "plaisio/form": "^0.55.0",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/table-overview": "^8.0.0",
    "plaisio/web-assets": "^3.1.0",
    "setbased/exception": "^2.3.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/console-type-script": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Form\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
